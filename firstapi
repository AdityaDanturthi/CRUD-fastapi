from fastapi import FastAPI, Path

app = FastAPI()

students = {
    
    1: {
     "name": 'FirstStudent'   
    },

    2: {
    "name": 'SecondStudent'
    }
}

@app.get("/get-student/{studentid}")
def get_student(studentid: int = Path(None, description="Please input the id of the student you would like to view")):
    return students[studentid]
